{
    "listeners": {
        "*:80": {
            "pass": "routes"
        }
    },
    "routes": [
        {
            "match": {
                "uri": "/upload/*"
            },
            "action": {
                "share": "/app/$uri"
            }
        },
        {
            "action": {
                "pass": "applications/fastapi"
            }
        }
    ],
    "applications": {
        "fastapi": {
            "type": "python 3.11",
            "path": "/",
            "working_directory": "/",
            "module": "app.main",
            "callable": "app"
        }
    }
}
